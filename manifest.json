  {
    "name": "Smpl",
    "version": "1.0",
    "description": "A simple new tab page.",
  	"chrome_url_overrides" : {
	  	"newtab": "smpl.html"
	  },	
    "permissions": ["activeTab", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "manifest_version": 2
  }